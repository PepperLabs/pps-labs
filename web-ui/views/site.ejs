  <script src="fullcalendar/dist/fullcalendar.min.js"></script>
  <script src="fullcalendar/dist/gcal.js"></script>
  <!-- </libs> -->
  <!-- <app-components> -->
  <script src="javascripts/app.js"></script>
  <script src="javascripts/controllers.js"></script>
  <script src="javascripts/directives.js"></script>
  <script src="/javascripts/user_module/userModule.js"></script>
  <script src="/javascripts/background.js"></script>
  <!-- </app-components> -->
</head>
<body ng-controller='MainCtrl' >
  <toast class="server-info" id="info" ng-show="info" ng-class="{'bg-success': infoGood, 'bg-danger': !infoGood}" toast-info="info"></toast>
  <section>
    <nav id="upperbar">
        <div class="general-menu">
            <a href="/#!/overview" class="item accueil"><span>Accueil</span></a>
            <a ui-sref="calendar()" class="item calendar" ng-class="{'selected': view=='calendar'}"><span>Calendrier</span></a>

        </div>
        <div class="userinfo">
            <div class="inner">
                <img class="pic" src="/img/user_placeholder.png"/>
                <div class="text text-lg">
                    <div>BONJOUR, <strong>{{user.firstname}} {{user.lastname}}</strong></div>
                    <span class="role">UTILISATEUR</span>
                </div>
                <div class="text text-sm">
                    <div>Hi, <strong>{{user.firstname}}</strong></div>
                    <span class="role">UTILISATEUR</span>
                </div>
            </div>
            <button class="logout" ng-click="$parent.logout()">
            </button>
        </div>

    </nav>
    <section ui-view></section>
    <div id='dragElem' style='z-index: 10000'></div>
  </section>
</body>
</html>

